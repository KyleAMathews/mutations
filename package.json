{
  "name": "@electric-sql/mutations",
  "version": "0.1.0",
  "description": "A proxy-based mutations library with optimistic updates and transaction support",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mutations",
    "state-management",
    "optimistic-updates",
    "transactions"
  ],
  "author": "ElectricSQL",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "proxy-compare": "^3.0.1"
  }
}
